# App
NODE_ENV=development
PORT=3000
API_URL=http://localhost:3001
CORS_ORIGIN=http://localhost:3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=strix_db
DB_USER=postgres
DB_PASSWORD=postgres

# Blockchain Networks
# Ethereum
ETH_MAINNET_RPC=https://eth-mainnet.alchemyapi.io/v2/your-api-key
ETH_TESTNET_RPC=https://eth-goerli.alchemyapi.io/v2/your-api-key
ETH_PRIVATE_KEY=your-private-key

# Solana
SOLANA_MAINNET_RPC=https://api.mainnet-beta.solana.com
SOLANA_TESTNET_RPC=https://api.testnet.solana.com
SOLANA_KEYPAIR_PATH=~/.config/solana/id.json

# Cosmos
COSMOS_RPC=https://rpc-cosmoshub.blockapsis.com
COSMOS_CHAIN_ID=cosmoshub-4
COSMOS_GAS_PRICES=0.025uatom

# Security
JWT_SECRET=your-jwt-secret
JWT_EXPIRATION=24h
ENCRYPTION_KEY=your-encryption-key

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis_password

# AI Services
OPENAI_API_KEY=your-openai-api-key
MODEL_CHECKPOINT_PATH=/path/to/models
TRAINING_DATA_PATH=/path/to/training/data

# Monitoring and Logging
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=debug
ELASTIC_APM_SERVER_URL=http://localhost:8200
ELASTIC_APM_SECRET_TOKEN=your-apm-token

# Feature Flags
ENABLE_GAS_OPTIMIZATION=true
ENABLE_SECURITY_SCANNER=true
ENABLE_AI_SUGGESTIONS=true

# AWS Services
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET=your-bucket-name

# External APIs
ETHERSCAN_API_KEY=your-etherscan-api-key
COINMARKETCAP_API_KEY=your-cmc-api-key

# CI/CD
DOCKER_REGISTRY=your-registry-url
DOCKER_USERNAME=your-username
DOCKER_PASSWORD=your-password

# Test Configuration
TEST_PRIVATE_KEY=test-private-key
TEST_WALLET_ADDRESS=test-wallet-address
FORK_BLOCK_NUMBER=latest